<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.java.bean.BookSite" table="booksite" catalog="java">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="mid" type="java.lang.Integer">
            <column name="mid" length="4" not-null="true"/>
        </property>
        <property name="number" type="java.lang.Integer">
            <column name="number" length="4" not-null="true"/>
        </property>
        <property name="time" type="timestamp">
            <column name="time"></column>
        </property>


    </class>
</hibernate-mapping>
